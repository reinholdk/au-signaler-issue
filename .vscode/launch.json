{
  "version": "0.2.0",
  "configurations": [
    {
      "command": "npm test",
      "name": "Run npm test",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "name": "Chrome",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:8080",
      "webRoot": "${workspaceRoot}/src",
      "userDataDir": "${workspaceRoot}/.chrome",
      "sourceMapPathOverrides": {
        "webpack:///./src/*": "${webRoot}/*"
      }
    }
  ]
}